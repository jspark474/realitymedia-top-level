    "version": "0.2.0",
    "configurations": [
      {
          "command": "npm start",
          "name": "Run npm start",
          "request": "launch",
          "type": "node-terminal",
          "env": {            
              "DB_FILE": "./db.sqlite",
              "ALLOWED_DOMAIN":"*",
              "ENABLE_SSO":"1",
              "NODE_ENV":"development",
              "PRIVATE_KEY_PATH":"./certs/privatekey.pem",
              "CHAIN_PATH":"./certs/certrequest.csr",
              "CERT_PATH":"./certs/certificate.pem",
              "HTTP_PORT":"3999",
          }
      }
    ]
}